{
    "name": "wunderio/client-fi-vake",
    "description": "vakehyva.fi Drupal website",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://git.drupalcode.org/project/elasticsearch_helper_views.git"
        },
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8",
            "exclude": [
                "drupal/ms_graph_api"
            ]
        },
        {
            "type": "git",
            "url": "https://git.drupalcode.org/issue/ms_graph_api-3261727.git"
        },
        {
            "name": "wunderio/ip_anonymisation_helper",
            "type": "vcs",
            "url": "https://github.com/wunderio/ip_anonymisation_helper.git"
        },
        {
            "type": "package",
            "package": {
                "name": "furf/jquery-ui-touch-punch",
                "version": "1.0",
                "type": "drupal-library",
                "source": {
                    "url": "https://github.com/furf/jquery-ui-touch-punch.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        }
    ],
    "require": {
        "ext-fileinfo": "*",
        "ext-json": "*",
        "ext-simplexml": "*",
        "composer/installers": "^2.0",
        "cweagans/composer-patches": "^1.7",
        "drupal/address": "^1.9",
        "drupal/addtoany": "^1.15",
        "drupal/admin_toolbar": "^3.0",
        "drupal/allowed_formats": "^1.3",
        "drupal/auto_entitylabel": "3.0-beta4",
        "drupal/autologout": "^1.3",
        "drupal/better_exposed_filters": "^5.0@beta",
        "drupal/block_field": "^1.0@RC",
        "drupal/ckeditor_quote": "^1.0",
        "drupal/components": "^3.0.0-beta3",
        "drupal/config_ignore": "^2.3",
        "drupal/config_readonly": "1.0-beta4",
        "drupal/config_split": "^2.0.0-rc1",
        "drupal/core-composer-scaffold": "^9.3",
        "drupal/core-recommended": "^9.3",
        "drupal/datalayer": "@beta",
        "drupal/datetimehideseconds": "^1.1",
        "drupal/easy_breadcrumb": "^2.0",
        "drupal/elasticsearch_helper": "7.x-dev",
        "drupal/elasticsearch_helper_views": "8.x-7.x-dev",
        "drupal/emulsify_twig": "^2.0",
        "drupal/entity_reference_tree": "^2.0",
        "drupal/entityreference_extensions": "^1.2",
        "drupal/extlink": "^1.6",
        "drupal/extra_field": "^2.1",
        "drupal/field_group": "^3.1",
        "drupal/focal_point": "^1.5",
        "drupal/form_options_attributes": "^1.2",
        "drupal/google_tag": "^1.4",
        "drupal/image_effects": "^3.1",
        "drupal/image_style_warmer": "^1.1",
        "drupal/key": "^1.15",
        "drupal/linkit": "^6.0@beta",
        "drupal/memcache": "^2.3",
        "drupal/menu_link_attributes": "^1.2",
        "drupal/menu_multilingual": "^1.0@alpha",
        "drupal/metatag": "^1.16",
        "drupal/migrate_plus": "6.0.1",
        "drupal/migrate_tools": "6.0",
        "drupal/monolog": "^2.0@beta",
        "drupal/ms_graph_api": "dev-3261727-d9-compatibility",
        "drupal/openid_connect": "^1.1",
        "drupal/openid_connect_windows_aad": "^1.3",
        "drupal/optional_end_date": "^1.2",
        "drupal/override_node_options": "^2.6",
        "drupal/paragraphs": "^1.14.0",
        "drupal/paragraphs_asymmetric_translation_widgets": "@beta",
        "drupal/pathauto": "^1.8",
        "drupal/redirect": "^1.6",
        "drupal/remove_entity_untranslatable_field_validation": "^1.1",
        "drupal/responsive_favicons": "^1.6",
        "drupal/scheduler": "^1.4",
        "drupal/scheduler_content_moderation_integration": "^1.3",
        "drupal/simplei": "^1.2",
        "drupal/smart_trim": "^1.3",
        "drupal/svg_image_field": "^2.1",
        "drupal/svg_sanitizer": "2.0",
        "drupal/translatable_menu_link_uri": "^2.0",
        "drupal/tvi": "^1.0@RC",
        "drupal/twig_field_value": "^2.0",
        "drupal/twig_tweak": "^3.0",
        "drupal/view_unpublished": "^1.0",
        "drupal/views_ajax_history": "^1.6",
        "drupal/warden": "^3.0@RC",
        "drupal/webform": "^6.1",
        "drupal/workbench_email": "^2.2",
        "drush/drush": "^11",
        "elasticsearch/elasticsearch": "~7.0",
        "furf/jquery-ui-touch-punch": "^1.0",
        "guzzlehttp/guzzle": "^6.5.4",
        "vlucas/phpdotenv": "^5.2",
        "webflo/drupal-finder": "^1.2",
        "wikimedia/composer-merge-plugin": "^2.0",
        "wunderio/drupal-ping": "^2.2",
        "wunderio/ip_anonymisation_helper": "dev-8.x-1.x"
    },
    "require-dev": {
        "drupal/core-dev": "^9.3",
        "drupal/devel": "^4.1",
        "drupal/migrate_devel": "2.0-alpha2",
        "drupal/stage_file_proxy": "^1.1",
        "kint-php/kint": "^4.0",
        "phpspec/prophecy-phpunit": "^2",
        "wunderio/code-quality": "^2.1"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "discard-changes": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "koodimonni/composer-dropin-installer": true,
            "phpro/grumphp": true,
            "drupal/core-composer-scaffold": true,
            "wikimedia/composer-merge-plugin": true
        }
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "merge-plugin": {
            "include": [
                "web/modules/contrib/webform/composer.libraries.json"
            ]
        },
        "composer-exit-on-patch-failure": true,
        "patches": {
            "drupal/emulsify_twig": {
                "Empty modifiers added if modifiers are NULL": "https://www.drupal.org/files/issues/2021-04-29/empty-modifiers-3157198-6.patch"
            },
            "drupal/ckeditor_quote": {
                "Use the <cite> element for the quote author": "https://www.drupal.org/files/issues/2020-10-26/ckeditor_quote-use-cite-tag-3178997-2.patch"
            },
            "drupal/core": {
                "Entity view/form mode formatter/widget settings have no translation UI": "https://www.drupal.org/files/issues/2020-11-17/2546212_165.patch",
                "Ajax exposed filters not working for multiple instances of the same Views block placed on one page": "https://www.drupal.org/files/issues/2022-01-03/3163299-49.patch"
            },
            "drupal/datalayer": {
                "Drupal 9 compatibility: Exception page because of incorrect path.alias_manager service name": "https://www.drupal.org/files/issues/2020-09-18/3171896-path_alias-manager.patch"
            },
            "drupal/google_tag": {
                "Aria-hidden attribute should not be used on <noscript>, also add title attribute": "https://www.drupal.org/files/issues/2021-11-15/add-title-to-iframe-3199758-10.patch"
            },
            "drupal/linkit": {
                "Multiple Linkit form elements": "https://www.drupal.org/files/issues/2021-03-23/3205105-2.patch"
            },
            "drupal/migrate_tools": {
                "Do not roll back everything with --sync option": "https://www.drupal.org/files/issues/2020-01-06/sync-id-hash-3104268-4.patch"
            },
            "drupal/paragraphs_asymmetric_translation_widgets": {
                "Suppress confusing \"You are not allowed to view this Paragraph\" message when editor creates a node translation.\r\n\tThis patch can be removed when drupal/remove_entity_untranslatable_field_validation is deprecated": "patches/paragraphs-remove-preview-info-message.patch"
            },
            "drupal/smart_trim": {
                "Carriage returns result in ascii encoded &#13;": "https://www.drupal.org/files/issues/2019-12-16/3101169_carriage-return-results-in-ascii-encoded.patch"
            },
            "drupal/warden": {
                "Support IP CIDR masks in allowed IPs": "https://www.drupal.org/files/issues/2019-01-07/warden-ip-range.txt"
            },
            "drupal/elasticsearch_helper_views": {
                "Remove EntityTranslationRenderTrait from RenderedEntity views field plugin": "https://www.drupal.org/files/issues/2020-10-20/remove-unnecessary-translationtrait-3177872-2.patch"
            },
            "drupal/metatag": {
                "Allow parent config to override empty values of more specific config": "https://www.drupal.org/files/issues/2020-04-08/metatag-parent_overrides-8-2044621-48.patch"
            }
        },
        "dropin-paths": {
            "web/": ["type:web-dropin", "package:wunderio/drupal-ping:_ping.php"]
        }
    }
}
